<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ImageRoundedCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerSizeBottomLeft">8dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
    </style>

    <style name="CutCorners" >
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
    </style>

    <style name="Widget.BottomNavigationView.ActiveIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="shapeAppearance">
            @style/ShapeAppearance.M3.Comp.NavigationBar.ActiveIndicator.Shape
        </item>
        <item name="android:color">@color/offWhite</item>
    </style>
</resources>